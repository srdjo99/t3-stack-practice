generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_URL")
  relationMode      = "prisma"
}

model Vote {
  id           String   @id @default(cuid())
  createdAt    DateTime @default(now())
  votedFor     Int
  votedAgainst Int
}
